{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schemas/ai-tool.schema.json",
  "title": "AI Tool Catalog Entry",
  "type": "object",
  "required": [
    "name",
    "slug",
    "website_url",
    "logo_url",
    "description",
    "category",
    "use_cases",
    "pros",
    "cons",
    "pricing_plans",
    "company_details",
    "reviews_and_ratings",
    "image",
    "best_for",
    "models",
    "technical_information",
    "key_differentiator",
    "case_studies",
    "vendor_details",
    "flags",
    "gallery",
    "tutorials_youtube",
    "qa",
    "official_video"
  ],
  "properties": {
    "website_url": {
      "type": "string",
      "format": "uri",
      "examples": [
        "https://exampletool.ai/"
      ]
    },
    "logo_url": {
      "type": "string",
      "format": "uri",
      "examples": [
        "https://cdn.exampletool.ai/logo.png"
      ]
    },
    "description": {
      "type": "string",
      "minLength": 50,
      "maxLength": 4000,
      "description": "Target ~300 words; limit ~4000 chars."
    },
    "category": {
      "type": "string",
      "description": "Primary category, e.g., text-to-image, code-assistant, RAG, voice, video, agent, data-labeling, search, analytics, devops, etc.",
      "examples": [
        "text-to-image"
      ]
    },
    "use_cases": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "title",
          "details"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "details": {
            "type": "string"
          },
          "industries": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    "pros": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "cons": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "pricing_plans": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "examples": [
              "Free",
              "Pro",
              "Enterprise"
            ]
          },
          "price": {
            "type": [
              "number",
              "string"
            ],
            "description": "Numeric or 'Contact sales'."
          },
          "currency": {
            "type": "string",
            "examples": [
              "USD",
              "EUR"
            ]
          },
          "billing_cycle": {
            "type": "string",
            "enum": [
              "monthly",
              "yearly",
              "one-time",
              "usage-based",
              "contact-sales"
            ]
          },
          "features": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "trial_available": {
            "type": "boolean"
          },
          "limits": {
            "type": "string"
          },
          "url": {
            "type": "string",
            "format": "uri"
          }
        }
      }
    },
    "company_details": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "founded_year": {
          "type": [
            "integer",
            "null"
          ],
          "minimum": 1800,
          "maximum": 2100
        },
        "hq_city": {
          "type": [
            "string",
            "null"
          ]
        },
        "hq_country": {
          "type": [
            "string",
            "null"
          ]
        },
        "team_size_range": {
          "type": [
            "string",
            "null"
          ],
          "examples": [
            "11-50",
            "51-200"
          ]
        },
        "funding": {
          "type": "object",
          "properties": {
            "total_raised": {
              "type": [
                "string",
                "null"
              ]
            },
            "last_round": {
              "type": [
                "string",
                "null"
              ]
            }
          }
        },
        "legal": {
          "type": "object",
          "properties": {
            "terms_url": {
              "type": [
                "string",
                "null"
              ],
              "format": "uri"
            },
            "privacy_url": {
              "type": [
                "string",
                "null"
              ],
              "format": "uri"
            },
            "security_url": {
              "type": [
                "string",
                "null"
              ],
              "format": "uri"
            }
          }
        },
        "contacts": {
          "type": "object",
          "properties": {
            "press_email": {
              "type": [
                "string",
                "null"
              ],
              "format": "email"
            },
            "support_email": {
              "type": [
                "string",
                "null"
              ],
              "format": "email"
            },
            "sales_email": {
              "type": [
                "string",
                "null"
              ],
              "format": "email"
            }
          }
        }
      }
    },
    "reviews_and_ratings": {
      "type": "array",
      "description": "Reviews & ratings from different social media or official website.",
      "items": {
        "type": "object",
        "required": [
          "source_name",
          "url"
        ],
        "properties": {
          "source_name": {
            "type": "string",
            "examples": [
              "G2",
              "Capterra",
              "Product Hunt",
              "Twitter/X",
              "YouTube",
              "Official site"
            ]
          },
          "source_type": {
            "type": "string",
            "examples": [
              "review_site",
              "social",
              "official"
            ]
          },
          "url": {
            "type": "string",
            "format": "uri"
          },
          "rating": {
            "type": [
              "number",
              "null"
            ],
            "minimum": 0,
            "maximum": 5
          },
          "rating_scale_max": {
            "type": [
              "number",
              "null"
            ],
            "minimum": 1
          },
          "rating_count": {
            "type": [
              "integer",
              "null"
            ],
            "minimum": 0
          },
          "excerpt": {
            "type": [
              "string",
              "null"
            ]
          },
          "reviewer": {
            "type": [
              "string",
              "null"
            ]
          },
          "date": {
            "type": [
              "string",
              "null"
            ],
            "format": "date"
          }
        }
      }
    },
    "image": {
      "type": "string",
      "format": "uri",
      "description": "Primary hero image URL."
    },
    "best_for": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "examples": [
        [
          "marketers",
          "designers",
          "SMBs"
        ]
      ]
    },
    "models": {
      "type": "array",
      "description": "Models used or offered by the tool.",
      "items": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "examples": [
              "SDXL",
              "GPT-4o",
              "Llama 3 70B"
            ]
          },
          "provider": {
            "type": [
              "string",
              "null"
            ]
          },
          "version": {
            "type": [
              "string",
              "null"
            ]
          },
          "modality": {
            "type": [
              "string",
              "null"
            ],
            "examples": [
              "text",
              "image",
              "audio",
              "video",
              "multimodal"
            ]
          },
          "context_window_tokens": {
            "type": [
              "integer",
              "null"
            ]
          },
          "input_formats": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "output_formats": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "fine_tuning_available": {
            "type": [
              "boolean",
              "null"
            ]
          },
          "notes": {
            "type": [
              "string",
              "null"
            ]
          }
        }
      }
    },
    "technical_information": {
      "type": "object",
      "properties": {
        "api_available": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "api_docs_url": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        },
        "sdk_languages": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "hosting_options": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "e.g., SaaS, on-prem, VPC."
        },
        "inference_endpoints": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "rate_limits": {
          "type": [
            "string",
            "null"
          ]
        },
        "data_retention_policy": {
          "type": [
            "string",
            "null"
          ]
        },
        "security": {
          "type": "object",
          "properties": {
            "encryption_at_rest": {
              "type": [
                "boolean",
                "null"
              ]
            },
            "encryption_in_transit": {
              "type": [
                "boolean",
                "null"
              ]
            },
            "pii_processing_supported": {
              "type": [
                "boolean",
                "null"
              ]
            },
            "compliance_certifications": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "subprocessors_url": {
              "type": [
                "string",
                "null"
              ],
              "format": "uri"
            }
          }
        },
        "integrations": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "key_differentiator": {
      "type": [
        "string",
        "array"
      ],
      "description": "Unique value proposition(s).",
      "items": {
        "type": "string"
      }
    },
    "case_studies": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "title"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "customer": {
            "type": [
              "string",
              "null"
            ]
          },
          "industry": {
            "type": [
              "string",
              "null"
            ]
          },
          "summary": {
            "type": [
              "string",
              "null"
            ]
          },
          "url": {
            "type": [
              "string",
              "null"
            ],
            "format": "uri"
          },
          "metrics": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "kpi",
                "value"
              ],
              "properties": {
                "kpi": {
                  "type": "string"
                },
                "value": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "vendor_details": {
      "type": "object",
      "properties": {
        "resellers": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "partners": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "support_url": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        },
        "support_email": {
          "type": [
            "string",
            "null"
          ],
          "format": "email"
        },
        "sla_url": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        }
      }
    },
    "flags": {
      "type": "object",
      "properties": {
        "is_open_source": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "has_free_tier": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "requires_login": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "has_api": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "gdpr_ready": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "hipaa_ready": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "coppa_ready": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "ai_claims_verified": {
          "type": [
            "boolean",
            "null"
          ],
          "description": "If crawler verified vendor claims with sources."
        }
      }
    },
    "gallery": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "url"
        ],
        "properties": {
          "url": {
            "type": "string",
            "format": "uri"
          },
          "caption": {
            "type": [
              "string",
              "null"
            ]
          }
        }
      }
    },
    "tutorials_youtube": {
      "type": "array",
      "description": "Tutorials â€“ YouTube watch links.",
      "items": {
        "type": "object",
        "required": [
          "url"
        ],
        "properties": {
          "title": {
            "type": [
              "string",
              "null"
            ]
          },
          "url": {
            "type": "string",
            "format": "uri",
            "pattern": "^(https?://)?(www\\.)?youtube\\.com/watch\\?v=[A-Za-z0-9_-]{6,}$"
          },
          "channel": {
            "type": [
              "string",
              "null"
            ]
          },
          "published_date": {
            "type": [
              "string",
              "null"
            ],
            "format": "date"
          }
        }
      }
    },
    "qa": {
      "type": "array",
      "minItems": 10,
      "description": "Q & A (at least 10 queries related to tool from anywhere).",
      "items": {
        "type": "object",
        "required": [
          "question",
          "answer"
        ],
        "properties": {
          "question": {
            "type": "string"
          },
          "answer": {
            "type": "string"
          },
          "source_url": {
            "type": [
              "string",
              "null"
            ],
            "format": "uri"
          }
        }
      }
    },
    "official_video": {
      "type": "string",
      "format": "uri",
      "description": "Official video from the website (landing or product page)."
    },
    "image_attribution": {
      "type": [
        "string",
        "null"
      ],
      "description": "Optional: attribution or license for images if required."
    },
    "last_crawled_at": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "description": "ISO timestamp when the crawler generated this entry."
    },
    "slug": {
      "type": "string",
      "description": "Unique identifier slug for each tool (UUID or human-readable key).",
      "examples": [
        "openai-chatgpt",
        "claude-anthropic",
        "a10e5e95-11dc-4f60-aa8b-f7cb9d4f223b"
      ]
    },
    "name": {
      "type": "string",
      "description": "Human-readable name of the AI tool.",
      "examples": [
        "ChatGPT",
        "Claude",
        "Midjourney",
        "Jasper",
        "DreamStudio"
      ]
    }
  },
  "additionalProperties": false
}